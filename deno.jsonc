{
  "name": "@deer/gfm",
  "version": "0.0.3",
  "license": "MIT",
  "exports": {
    ".": "./mod.ts",
    "./style": "./style.ts"
  },
  "workspace": ["./style"],
  "nodeModulesDir": "auto",
  "imports": {
    "unified": "npm:unified@^11.0.5",
    "remark-parse": "npm:remark-parse@^11",
    "remark-gfm": "npm:remark-gfm@^4.0.1",
    "remark-frontmatter": "npm:remark-frontmatter@^5",
    "remark-math": "npm:remark-math@^6",
    "remark-emoji": "npm:remark-emoji@^5.0.2",
    "rehype-github-alerts": "npm:rehype-github-alerts@^4.2.0",
    "remark-rehype": "npm:remark-rehype@^11.1.2",
    "rehype-slug": "npm:rehype-slug@^6",
    "rehype-autolink-headings": "npm:rehype-autolink-headings@^7.1.0",
    "rehype-sanitize": "npm:rehype-sanitize@^6",
    "rehype-stringify": "npm:rehype-stringify@^10.0.1",
    "rehype-starry-night": "npm:rehype-starry-night@^2.2.0",
    "rehype-highlight": "npm:rehype-highlight@^7.0.2",
    "rehype-katex": "npm:rehype-katex@^7.0.1",
    "rehype-raw": "npm:rehype-raw@^7",
    "@wooorm/starry-night": "npm:@wooorm/starry-night@^3.9.0",
    "hast-util-to-string": "npm:hast-util-to-string@^3.0.1",
    "hast-util-heading-rank": "npm:hast-util-heading-rank@^3",
    "unist-util-visit": "npm:unist-util-visit@^5.1.0",
    "mdast-util-to-string": "npm:mdast-util-to-string@^4",
    "github-slugger": "npm:github-slugger@^2",
    "yaml": "npm:yaml@^2.8.2",
    "mdast": "npm:@types/mdast@^4.0.4",
    "hast": "npm:@types/hast@^3.0.4",
    "@std/assert": "jsr:@std/assert@^1.0.18",
    "@std/testing": "jsr:@std/testing@^1.0.17",
    "@astral/astral": "jsr:@astral/astral@^0.5.5"
  },
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "dom.asynciterable",
      "deno.ns"
    ]
  },
  "publish": {
    "exclude": [
      ".github/",
      "bench.ts",
      "CLAUDE.md",
      "deno.lock",
      "docs/",
      "mod_test.ts",
      "ROADMAP.md",
      "scripts/",
      "style/",
      "test/"
    ]
  },
  "tasks": {
    "ok": "deno fmt --check && deno lint && deno check && deno task test",
    "test": "rm -rf .coverage && deno test -A --coverage=.coverage",
    "bench": "deno bench -A",
    "gen:style": "deno run -A style/build.ts && deno fmt style.ts",
    "serve": "deno run -A test/server.ts"
  },
  "allowScripts": ["npm:@parcel/watcher@2.5.6"]
}
